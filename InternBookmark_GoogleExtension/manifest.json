{
    "manifest_version": 3,
    "name": "Intern Info Crawler",
    "version": "1.0",
    "description": "An extension to crawl infos from 104/cakeresume/skyline as bookmarks",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "host_permissions": [
      "http://127.0.0.1:5000/"
    ],
  
    "background": {
      "service_worker": "background/background.js"
    },
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "128": "images/open-book.png"
      }
    },
    "icons": {
      "128": "images/open-book.png"
    }
  }
  


  